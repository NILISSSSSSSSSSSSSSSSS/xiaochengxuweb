{
  "extends": ["taro"],
  "rules": {
    "no-unused-vars": ["error", {
      // "varsIgnorePattern": "Taro",
      "args": "none"
    }],
    "react/jsx-filename-extension": [1, {
      "extensions": [".js", ".jsx", ".tsx"]
    }],
    "jsx-quotes": ["error", "prefer-double"]
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "useJSXTextNode": true,
    "project": "./tsconfig.json"
  }
}
